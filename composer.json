{
  "name": "lukasbecker/kirby3-mix",
  "description": "Extends the Kirby css() and js() helpers with asset hashes, generated by Laravel Mix.",
  "type": "kirby-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas Becker",
      "email": "lukas@lukas-becker.eu"
    }
  ],
  "require": {
    "getkirby/composer-installer": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "LukasBecker\\Mix\\": "classes/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
